  "dag": {
    "inputs": {
      "rpc": {
        "trigger": {
          "methods": [
            "GET"
          ],
          "path": "/trigger"
        }
      }
    },
    "nodes": [
      {
        "#": "RPC trigger",
        "input": {
          "bucket": "trigger"
        },
        "implementation": {
          "javascript": "server/rpc.js"
        },
        "outputs": {
          "trigger-st": {},
          "api_rpc": {}
        }
      },
      {
        "#": "Cron trigger",
        "implementation": {
          "javascript": "server/cron.js",
          "when": {
            "interval": 180
          }
        },
        "outputs": {
          "trigger-st": {}
        }
      },
      {
        "#": "Emit Node",
        "input": {
          "bucket": "trigger-st"
        },
        "implementation": {
          "javascript": "server/node1.js"
        },
        "outputs": {
          "forensics-st": {}
        }
      },
