{
  "name": "crontest",
  "display-name": "Crontest Sift",
  "description": "",
  "author": "",
  "version": "1.0.0",
  "icon": "assets/blueprint.svg",
  "interfaces": {
    "summary": {
      "controller": "dist/js/controller.umd-es2015.min.js"
    }
  },
  "dag": {
    "inputs": {
      "emails": {},
      "slack-bot": {},
      "webhooks": {},
      "rpc": {}
    },
    "nodes": [
      {
        "#": "First Node",
        "implementation": {
          "javascript": "server/node1.js",
          "when": {
            "interval": 1000
          }
        },
        "outputs": {
          "forensics-st":{ "discardValue": true}
        }
      },
      {
        "#": "Update Forensics Index",
        "input": {
          "bucket": "forensics-st",
          "select": "/"
        },
        "implementation": {
          "go": "server/gow/emailload/load.go",
          "sandbox": "quay.io/redsift/sandbox-go-dmarc-insights:v1.1",
          "qos": {
            "large-storage": [
              "rocksdb_store_forensics:rw"
            ]
          }
        }
      }
    ],
    "stores": {
      "forensics-st": {
        "key$schema": "string/string"
      }
    },
    "outputs": {}
  }
}